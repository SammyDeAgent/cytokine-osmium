<body>
	<%- include('component/navbar',{selected: 'home'}) %>

	<div class="d-flex row no-gutters">

		<div class="col-lg-8">
			<div class="card m-3">
				<div class="card-body">
					<h3 class="card-title">Welcome to Osmium!</h3>
					<p class="card-text">
						Upcoming team finder web app.
					</p>
					<% if(logged == 1) {%>
					<p>Hello, <strong><%=user_name%></strong> ( <%=e_mail%> ) </p>
					<p>Your account has been verified!</p>
					<% } %>
				</div>
			</div>

			<div class="card m-3">
				<img class="card-img-top" src="img/cyto_banner.png">
				<div class="card-body">
					<h4 class="card-title">Site under construction</h4>
					<p class='card-text'>
						Stay tuned for more functionalities!<br>
						Developed by <a href="https://github.com/SammyDeAgent" target="_blank">Sammy</a>
					</p>
					<form action="/debug" method="GET">
						<button class="btn btn-secondary" type="submit">Debug Page</button>
					</form>
				</div>
			</div>

		</div>

		<div class="col-lg-4">
			<div class="card mt-3 mb-3 mr-3" style="overflow:scroll; max-height: 645px;">
				<div class="card-body">
					<h4 class="card-title">Update Logs - <strong>Beta <%= patch.version %></strong></h4>
					<p>[<%= patch.date %>] - <%= patch.author %></p>
					<a href="/patch" class="btn btn-block btn-info">Go to Update Page</a>
					<hr class="bg-muted">
					<div class="alert alert-warning" role="alert">
						<strong>Heads up!</strong> Beta testing is live!
					</div>
					<ul class="list-group">
						<h3> New Additions </h3>
						<% for(let j = 0; j < patch.patch.addition.length; j++) { %>
						<li class="list-group-item "> <%= patch.patch.addition[j] %> </li>
						<% } %>

						<h3 class="mt-2"> Misc Updates </h3>
						<% for(let k = 0; k < patch.patch.updates.length; k++) { %>
						<li class="list-group-item "> <%= patch.patch.updates[k] %> </li>
						<% } %>

						<h3 class="mt-2"> Upcoming Patches </h3>
						<% for(let l = 0; l < patch.patch.upcoming.length; l++) { %>
						<li class="list-group-item "> <%= patch.patch.upcoming[l] %> </li>
						<% } %>

						<h3 class="mt-2"> Incomplete and Issues </h3>
						<% for(let m = 0; m < patch.patch.issues.length; m++) { %>
						<li class="list-group-item "> <%= patch.patch.issues[m] %> </li>
						<% } %>
					</ul>
				</div>
			</div>
		</div>
	</div>

	<%- include('component/footer',{}) %>

	<!-- Visual Scripts -->
	<script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"
		integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous">
	</script>
